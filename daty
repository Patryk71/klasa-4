const dzis = new Date();
const przycisk = document.querySelector('button');
function data_fun(){
    let time1 = new Date(document.querySelector("#dates").value).getTime();
    let time2 = new Date(document.querySelector("#dates2").value).getTime();
    wynik = Math.round((time2-time1)/1000/60/60/24);
    if (wynik <0){
        document.querySelector('.wynik').innerHTML = `od tego dnia mineło ${-wynik} dni`;
    }else if(wynik == 0){
        document.querySelector('.wynik').innerHTML = `to ten sam dzień`;
    }else{
        document.querySelector('.wynik').innerHTML = `do tego dnia brakuje ${wynik} dni`;
    }
};
przycisk.addEventListener('click',data_fun);




